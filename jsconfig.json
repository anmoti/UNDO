{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "bundler",
        "lib": [
            "ESNext",
            "DOM",
            "DOM.Iterable"
        ],
        "checkJs": true,
        "baseUrl": ".",
        "paths": {
            "@hotwired/stimulus": [
                "./vendor/javascript/@hotwired/stimulus"
            ],
            "@hotwired/stimulus-loading": [
                "./vendor/bundle/ruby/3.4.0/gems/stimulus-rails-1.3.4/app/assets/javascripts/stimulus-loading"
            ],
            "controllers/*": [
                "./app/javascript/controllers/*"
            ]
        }
    },
    "include": [
        "./app/javascript/**/*.js"
    ]
}
